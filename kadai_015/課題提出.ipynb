        "class Human :\n",
        "      def __init__(self, name, age):\n",
        "        # 属性を定義する\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "        # メソッドを定義する\n",
        "      def printinfo(self):\n",
        "           print(self.name,self.age)\n",
        "\n",
        "# インスタンス化する\n",
        "human = Human(36,\"侍太郎\" )\n",
        "\n",
        "# 属性にアクセスし、値を出力する\n",
        "human.printinfo()\n"
      ],
